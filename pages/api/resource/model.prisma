model Track {
  id        String   @id @default(uuid())
  resources Resource[]
}

model Resource {
    id          String @id @default(uuid())
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
    url         String
    text        String
    image       String
    track       Track @relation(fields: [trackId], references: [id]) 
    trackId     String
}